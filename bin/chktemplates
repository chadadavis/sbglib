#!/usr/bin/env perl

use strict;
use SBG::Eval;
use SBG::DomainIO;
use SBG::Domain;

# Command line:
# ./chktemplates 2j57_templates.csv

my $f = ".t.dom";


while (<>) {
    chomp;
    next unless $_;
    my $id = 0;
    unlink $f;
    open my $fh, ">$f";
    my ($id1, $d1, $id2, $d2) = /(\S+)\s+(\{.*?\})/g;
    print $fh " $id1", $id++, " $d1\n";
    print $fh " $id2", $id++, " $d2\n";
    close $fh;
    print `cat $f`;
    `tras $f`;
}
unlink $f;
